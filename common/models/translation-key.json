{
  "name": "TranslationKey",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "keyString": {
      "type": "string",
      "dataType": "text",
      "required": true,
      "length": 65534
    },
    "isPlural": {
      "type": "boolean",
      "required": false,
      "default": false
    },
    "projectId": {
      "type": "number",
      "required": true
    },
    "namespaceId": {
      "type": "number",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "translations": {
      "type": "hasMany",
      "model": "Translation",
      "foreignKey": "translationsKeyId"
    },
    "project": {
      "type": "belongsTo",
      "model": "Project",
      "foreignKey": "projectId"
    },
    "namespace": {
      "type": "belongsTo",
      "model": "Namespace",
      "foreignKey": "namespaceId"
    }
  },
  "acls": [],
  "methods": {}
}
